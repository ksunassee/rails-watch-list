<div class="container"> 
	<div> 
		<h1>All lists</h1>
		<% @lists.each do |list| %>
		  <li><%= link_to list.name, list %></li>
		  <%= link_to "Delete List", list, method: :delete, data: { confirm: "Are you sure?" } %></li>
		<% end %>
	</div>
	<br><br>
	<div>
		<%= link_to "Create a new list" , new_list_path %>

	</div>
</div>